{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "prestart": "set NODE_ENV=development && gulp env",
    "start": "polymer serve",
    "build": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "postbuild": "set NODE_ENV=production && gulp env",
    "postbuild:static": "set NODE_ENV=production && gulp env",
    "preserve": "set NODE_ENV=production && gulp env",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-dropdown": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-input": "^3.2.0",
    "@polymer/polymer": "^3.3.1",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "jquery": "^3.4.1",
    "lit-element": "^2.2.1",
    "mte-calendar": "git+https://github.com/AlderMatusO/mte-calendar.git",
    "polymer-modulizer": "^0.4.3",
    "popper.js": "^1.16.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "simple-dropdown": "github:SimpleElements/simple-dropdown",
    "sweetalert2": "^9.7.1"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.4.1",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.20.0",
    "wct-mocha": "^1.0.0"
  }
}
